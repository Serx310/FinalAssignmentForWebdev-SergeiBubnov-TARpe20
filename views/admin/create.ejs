<%-include('../partials/partialtime', {DocumentTitle:'Welcome, Mr. Administrator!'})%>

  <h1>New skill</h1>

  <form action="/NewSkill" method="POST">
      <div class="form-group">
        <label for="exampleInputEmail1">Title</label>
        <input type="text" name="title" class="form-control" placeholder="Title">
      </div>
      <label>Skill type</label>
      <select name="skillType" class="form-select">
        <option value="Soft" selected>Soft</option>
        <option value="Technical">Technical</option>
      </select>
      <button class="btn btn-primary mt-3" type="submit" class="btn btn-primary">Save</button>
    </form>

  <h1>New education</h1>

  <form action="/NewEducation" method="POST">
      <div class="form-group">
        <label for="exampleInputEmail1">School name</label>
        <input type="text" name="schoolName" class="form-control" placeholder="School name">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1">Type of education</label>
        <input type="text" name = "educationType" class="form-control"  placeholder="Description">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1">Date of graduation</label>
        <input type="date" name = "graduated" class="form-control" placeholder="Date of graduation">
      </div>
      <button class="btn btn-primary mt-3"  type="submit" class="btn btn-primary">Save</button>
    </form> 
</div>
<%-include('../partials/footer')%>